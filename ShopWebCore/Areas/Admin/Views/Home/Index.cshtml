                <!-- Chat box -->
                <div class="scroll-div">
                    <div class="">
                        <div class="box-header ui-sortable-handle" style="text-align:center">
                            <i class="fa fa-comments-o"></i>
                            <h3 class="box-title">آخرین کامنت ها</h3>
                        </div>
                        <div class="box-body chat" id="chat-box" style="overflow: visible; width: auto; height: 250px;">
                            <!-- chat item -->
                            @foreach (var Message in _generic.GetAll().Include(m => m.User).OrderByDescending(m => m.Id).Take(20).ToList())
                            {
                                <div class="item">
                                    <img src="dist/img/user2-160x160.jpg" alt="user image" class="offline">

                                    <p class="message">
                                        <a href="#" class="name">
                                            <small class="text-muted pull-left"><i class="fa fa-clock-o"></i> @Message.CreationDate.ToShortTimeString()</small>
                                            @Message.User.FullName
                                        </a>
                                        @Message.Comment
                                    </p>
                                </div>
                            }
                            <!-- /.item -->
                        </div>
                    </div>
                </div>
                <!-- /.box (chat box) -->
                <!-- TO DO List -->
                <div class="box box-primary">
